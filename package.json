{
  "name": "awell-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-types": "graphql-codegen --config codegen.js",
    "prebuild": "npm run generate-types",
    "build": "next build",
    "postbuild": "node ./scripts/build-algolia-search/build-algolia-search.js",
    "start": "next start",
    "predev": "npm run generate-types",
    "dev": "next dev",
    "pre-commit": "npm run generate-types && npm run ts && next lint --fix",
    "prepare": "husky install",
    "ts": "tsc --noEmit --incremental",
    "lint": "next lint",
    "lint:fix": "next lint --fix .",
    "clean": "rm -rf ./.next && rm ./tsconfig.tsbuildinfo",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@apollo/client": "^3.9.11",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^1.0.6",
    "@markprompt/core": "^0.26.3",
    "@markprompt/react": "^0.40.7",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "algoliasearch": "^4.23.3",
    "axios": "^1.7.4",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "glob": "^7.1.7",
    "graphiql": "^3.2.0",
    "graphql": "^16.8.1",
    "gray-matter": "^4.0.3",
    "libphonenumber-js": "^1.10.60",
    "lodash": "^4.17.21",
    "next": "^14.2.5",
    "next-mdx-remote": "^4.4.1",
    "next-transpile-modules": "^9.1.0",
    "prism-react-renderer": "^2.3.1",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.2",
    "react-markdown": "^8.0.7",
    "recharts": "^2.1.10",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@graphql-codegen/typescript-operations": "4.2.0",
    "@graphql-codegen/typescript-react-apollo": "4.3.0",
    "@next/bundle-analyzer": "^13.5.6",
    "@tailwindcss/forms": "^0.5.7",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.9.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.5",
    "postcss-import": "^14.1.0",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
